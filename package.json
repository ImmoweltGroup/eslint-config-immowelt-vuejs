{
  "name": "@immowelt/eslint-config-immowelt-vuejs",
  "version": "0.0.0",
  "description": "ESLint shareable vuejs config",
  "main": "index.js",
  "repository": "https://github.com/ImmoweltGroup/eslint-config-immowelt-vuejs.git",
  "author": "Kevin Ruff <kevin.ruff@immonet.de>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "dependencies": {
    "@immowelt/eslint-config-immowelt-base": "1.1.0",
    "eslint": "^5.6.1",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "flow-bin": "^0.83.0"
  },
  "devDependencies": {
    "semantic-release": "^15.9.17"
  },
  "scripts": {
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        }
      ]
    }
  },
  "keywords": [
    "eslint",
    "config",
    "immowelt",
    "vuejs"
  ]
}
